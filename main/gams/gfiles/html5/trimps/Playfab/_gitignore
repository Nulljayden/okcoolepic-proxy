import json

def load_title_data(file_name):
    with open(file_name) as f:
        title_data = json.load(f)
    return title_data

def process_title_data(title_data):
    # process the title data here
    pass

def main():
    file_name = 'testTitleData.json'
    title_data = load_title_data(file_name)
    processed_data = process_title_data(title_data)

if __name__ == '__main__':
    main()
